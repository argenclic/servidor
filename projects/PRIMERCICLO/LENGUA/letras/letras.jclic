    <style>
     <color background="0xFFFFFF" />
    </style>
    <shaper class="@Rectangular" cols="1" rows="1" />
   </cells>
  </activity>
  <activity class="@panels.Explore" name="primera.ass" code="">
   <description />
   <messages>
    <cell type="initial">
     <style>
      <color background="0x8080FF" />
     </style>
     <p>¿QUÉ HACEMOS?</p>
