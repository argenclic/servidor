    <style name="target" base="default" foreground="0x0000FF" target="true" />
    <style name="targetError" base="target" foreground="0xFF0000" />
    <section>
     <p background="0xFFFFFF" family="Arial">
      <text background="0xFFFFFF" family="Arial">                       </text>
      <cell width="200" height="40">
       <style>
        <font family="Arial" size="17" bold="true" />
        <color background="0x6600FF" />
       </style>
       <p>Las palabras agudas</p>
      </cell>
     </p>
     <p background="0xFFFFFF" family="Arial">
      <text background="0xFFFFFF" family="Arial">Las palabras, según la posición de su sílaba </text>
      <target>
       <answer>tónica</answer>
       <optionList>
        <option>primera</option>
        <option>átona</option>
        <option>tónica</option>
        <option>última</option>
       </optionList>
       <info mode="onError">
        <cell width="100" height="40">
         <style>
          <color background="0x6600FF" />
         </style>
         <p>es la sílaba con mayor golpe de voz</p>
        </cell>
       </info>
       <text>tónica</text>
      </target>
      <text background="0xFFFFFF" family="Arial">, pueden ser </text>
      <target>
       <answer>llanas</answer>
       <answer>esdrújulas</answer>
       <answer>agudas</answer>
       <response fill="_" length="6" maxLength="6" />
       <text>agudas</text>
      </target>
      <text background="0xFFFFFF" family="Arial">, </text>
      <target>
       <answer>agudas</answer>
       <answer>esdrújulas</answer>
       <answer>llanas</answer>
       <response fill="_" length="6" maxLength="6" />
       <text>llanas</text>
      </target>
      <text background="0xFFFFFF" family="Arial"> o </text>
      <target>
       <answer>agudas</answer>
       <answer>llanas</answer>
       <answer>esdrújulas</answer>
       <response fill="_" length="10" maxLength="10" />
       <text>esdrújulas</text>
      </target>
      <text background="0xFFFFFF" family="Arial">. La sílaba </text>
      <target>
       <answer>tónica</answer>
       <optionList>
        <option>principal</option>
        <option>primera</option>
        <option>tónica</option>
       </optionList>
       <info mode="onError">
        <cell width="100" height="40">
         <style>
          <color background="0x6600FF" />
         </style>
         <p>ya lo has escrito antes</p>
        </cell>
       </info>
       <text>tónica</text>
      </target>
      <text background="0xFFFFFF" family="Arial">, en algunas palabras, se marca con un signo llamado </text>
      <target>
       <answer>tilde</answer>
       <optionList>
        <option>punto</option>
        <option>puntillo</option>
        <option>tilde</option>
        <option>punto y coma</option>
       </optionList>
       <info mode="onError">
        <cell width="100" height="37">
         <style>
          <color background="0x6600FF" />
         </style>
         <p>empieza por t</p>
        </cell>
       </info>
       <text>tilde</text>
      </target>
      <text background="0xFFFFFF" family="Arial"> (´) sobre una de sus vocales</text>
     </p>
     <p background="0xFFFFFF" family="Arial">
      <text background="0xFFFFFF" family="Arial">Las palabras agudas son las que llevan la sílaba tónica en la </text>
      <target>
       <answer>última</answer>
       <optionList>
        <option>primera</option>
        <option>penúltima</option>
        <option>antepenúltima</option>
        <option>última</option>
       </optionList>
       <info mode="onError">
        <cell width="100" height="40">
         <style>
          <color background="0x6600FF" />
         </style>
         <p>piénsalo mejor</p>
        </cell>
       </info>
       <text>última</text>
      </target>
      <text background="0xFFFFFF" family="Arial"> posición.</text>
     </p>
     <p background="0xFFFFFF" family="Arial">
      <text background="0xFFFFFF" family="Arial">Llevan </text>
      <target>
       <answer>tilde</answer>
       <response fill="_" length="5" maxLength="5" />
       <info mode="onError">
        <cell width="100" height="40">
         <style>
          <color background="0x6600FF" />
         </style>
         <p>ya lo has escrito antes</p>
        </cell>
       </info>
       <text>tilde</text>
      </target>
      <text background="0xFFFFFF" family="Arial"> cuando terminan en </text>
      <target>
       <answer>-n, en -s o en vocal</answer>
       <optionList>
        <option>-r,en -z o en d</option>
        <option>-n, en -s o en vocal</option>
        <option>siempre</option>
       </optionList>
       <info mode="onError">
        <cell width="100" height="40">
         <style>
          <color background="0x6600FF" />
         </style>
         <p>balcón, compás, sofá</p>
        </cell>
       </info>
       <text>-n, en -s o en vocal</text>
      </target>
     </p>
     <p background="0xFFFFFF" family="Arial" />
    </section>
   </document>
