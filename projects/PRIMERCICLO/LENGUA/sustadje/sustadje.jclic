        <option>ROJAS</option>
        <option>MAYOR</option>
        <option>FRÍA</option>
        <option>RÁPIDA</option>
       </optionList>
       <text>MAYOR</text>
      </target>
      <text> DE LUIS SE CASÓ</text>
     </p>
