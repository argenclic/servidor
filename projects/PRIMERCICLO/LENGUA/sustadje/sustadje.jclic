   <checkButton>HACÉ CLIC ACÁ CUANDO FINALICES</checkButton>
   <document>
    <style name="default" size="17" background="0xFFFFFF" tabWidth="16" family="Arial" bold="false" foreground="0x000000" italic="false" />
    <style name="target" base="default" foreground="0x0000FF" background="0xC0C0C0" target="true" />
    <style name="targetError" base="target" background="0xFF0000" foreground="0xFFFFFF" />
    <section>
     <p size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false">
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false">DOROTEA Y SU PERRITO SON ARRASTRADOS POR UN </text>
      <target>ENORME</target>
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false"> REMOLINO DE VIENTO QUE DESPEGA SU CASA DEL SUELO Y LA LLEVA VOLANDO HASTA UNA </text>
      <target>EXTRAÑA</target>
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false"> REGIÓN. ALLÍ SE ENCUENTRA CON UNOS PERSONAJES </text>
      <target>RARÍSIMOS</target>
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false">:</text>
     </p>
     <p size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false">
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false">MIENTRAS ESTABA ALLI DE PIE, MIRANDO </text>
      <target>ÁVIDAMENTE</target>
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false"> EL </text>
      <target>EXTRAÑO</target>
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false"> Y </text>
      <target>HERMOSO</target>
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false"> PAISAJE, OBSERVÓ QUE SE LE ACERCABA UN GRUPO DE GENTE MUY </text>
      <target>RARA</target>
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false">.</text>
     </p>
     <p size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false">
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false">ERAN TRES HOMBRES Y UNA MUJER, Y VESTÍAN TODOS CON EXTRAVAGANCIA.  LLEVABAN SOMBREROS </text>
      <target>REDONDOS</target>
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false"> QUE TERMINABAN EN UNA PUNTA </text>
      <target>AGUDA</target>
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false">, CON UNAS CAMPANILLAS EN LAS ALAS, QUE SONABAN DULCEMENTE CUANDO SE MOVÍAN.</text>
     </p>
    </section>
   </document>
