  <title>Sílabas</title>
  <revision description="Creación del paquete de actividades en Clic 3.0" date="10/20/97" />
  <revision description="Traducción Español" date="6/6/01">
   <author name="José Pedro Roda Martínez" mail="03010247@centres.cult.gva.es" />
