  <activity class="@panels.InformationScreen" name="defini01.ass">
   <description>
    <p>DEFINICIÓN Y CLASIFICACIÓN</p>
   </description>
   <messages>
    <cell type="initial">
     <style>
      <font family="Arial" size="17" bold="true" />
