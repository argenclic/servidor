    </container>
    <window bgColor="0xFF0000" border="true">
     <size width="480" height="150" />
    </window>
   </settings>
   <document>
    <style name="default" background="0xFFFFFF" family="Arial" size="17" italic="true" tabWidth="16" foreground="0x000000" bold="true" />
    <style name="target" base="default" background="0xC0C0C0" foreground="0x0000FF" target="true" />
    <style name="targetError" base="target" background="0xFF0000" foreground="0xFFFFFF" />
    <section>
     <p background="0xFFFFFF" family="Arial" size="17" italic="true" bold="true" foreground="0x000000">
      <text background="0xFFFFFF" family="Arial" size="17" italic="true" bold="true" foreground="0x000000">La montaña era </text>
      <target>
       <answer>altísima</answer>
       <response fill="_" length="8" maxLength="8" />
       <text>altísima</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="true" bold="true" foreground="0x000000"> y cubierta de nieves </text>
      <target>
       <answer>perpetuas</answer>
       <response fill="_" length="9" maxLength="9" />
       <text>perpetuas</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="true" bold="true" foreground="0x000000">. Los </text>
      <target>
       <answer>intrépidos</answer>
       <response fill="_" length="10" maxLength="10" />
       <text>intrépidos</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="true" bold="true" foreground="0x000000"> escaladores ascendían lentamente. Había valles </text>
      <target>
       <answer>profundos</answer>
       <response fill="_" length="9" maxLength="9" />
       <text>profundos</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="true" bold="true" foreground="0x000000"> con simas </text>
      <target>
       <answer>escalofriantes</answer>
       <response fill="_" length="14" maxLength="14" />
       <text>escalofriantes</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="true" bold="true" foreground="0x000000">. La </text>
      <target>
       <answer>dura</answer>
       <response fill="_" length="4" maxLength="4" />
       <text>dura</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="true" bold="true" foreground="0x000000"> pared de la montaña dificultaba la </text>
      <target>
       <answer>difícil</answer>
       <response fill="_" length="7" maxLength="7" />
       <text>difícil</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="true" bold="true" foreground="0x000000"> ascensión.</text>
     </p>
    </section>
   </document>
