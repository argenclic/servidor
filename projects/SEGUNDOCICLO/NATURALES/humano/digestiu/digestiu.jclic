<?xml version="1.0" encoding="UTF-8"?>
<JClicProject name="digestiu" version="0.1.3">
 <settings>
  <title>El cuerpo humano - 1</title>
  <revision description="Creación del paquete de actividades en Clic 3.0" date="5/5/97" />
  <revision description="Traducción Español" date="5/5/97">
   <author name="Rafael Rodríguez Martín" mail="rrodri4@platea.pntic.mec.es" />
  </revision>
  <revision description="Conversión de Clic3 a JClic" date="7/11/02">
   <author name="Equip Clic" mail="clic@pie.xtec.es" organization="Subdirecció General de Tecnologies de la Informació (SGTI)" />
  </revision>
  <author name="Seminari d'Informàtica Educativa del Pla d'Urgell" />
  <language>es</language>
  <description>
   <p>Paquete de actividades sobre el cuerpo humano: los aparatos digestivo, excretor, locomotor, respiratorio, reproductor y circulatorio. Este material corresponde a la unidad de programación &lt;B&gt;&lt;A HREF="http://www.xtec.es/recursos/curricul/experim/coshuma.htm"  TARGET="_blank"&gt;"El cos humà"&lt;/A&gt;&lt;/B&gt;.</p>
  </description>
  <descriptors area-codes="exp" level="Primaria, Secundaria">anatomía</descriptors>
  <meta_langs>es</meta_langs>
  <descriptions>
   <description language="es">
    <p>Paquete de actividades sobre el cuerpo humano: los aparatos digestivo, excretor, locomotor, respiratorio, reproductor y circulatorio. Este material corresponde a la unidad de programación &lt;B&gt;&lt;A HREF="http://www.xtec.es/recursos/curricul/experim/coshuma.htm"  TARGET="_blank"&gt;"El cos humà"&lt;/A&gt;&lt;/B&gt;.</p>
   </description>
  </descriptions>
  <license type="by-nc-sa" url="https://creativecommons.org/licenses/by-nc-sa/4.0" />
 </settings>
 <sequence>
  <item id="digestiu.pac" name="digestiu/digestiu.puz">
   <jump action="RETURN" id="back" />
  </item>
  <item name="digestiu/digestiu.ass" />
  <item name="digestiu/digestiu.sop">
   <jump action="RETURN" id="forward" />
  </item>
 </sequence>
 <activities>
  <activity class="@associations.SimpleAssociation" name="digestiu\digestiu.ass">
   <messages>
    <cell type="initial">
     <style>
      <font family="Arial" size="13" bold="true" />
     </style>
     <p>Relaciona los órganos del aparato digestivo</p>
    </cell>
    <cell type="final">
     <style>
      <font family="Arial" size="13" bold="true" />
     </style>
     <p>¡ Muy bien !</p>
    </cell>
   </messages>
   <settings margin="8" report="true" reportActions="false">
    <helpWindow showSolution="true" />
    <container bgColor="0xC0C0C0">
     <counters time="true" actions="true" score="true" />
    </container>
    <window bgColor="0xC0C0C0" border="true" />
   </settings>
   <cells rows="2" cols="2" cellWidth="200.0" cellHeight="200.0" border="true" id="primary">
    <style>
     <font family="Arial" size="13" bold="true" />
     <color background="0xFFFFFF" />
    </style>
    <shaper class="@Rectangular" cols="2" rows="2" />
    <cell>
     <p>esófago y estómago</p>
    </cell>
    <cell>
     <p>hígado y vesícula</p>
    </cell>
    <cell>
     <p>páncreas</p>
    </cell>
    <cell>
     <p>intestinos</p>
    </cell>
   </cells>
   <cells rows="2" cols="2" cellWidth="200.0" cellHeight="200.0" border="true" id="secondary">
    <style>
     <font family="Arial" size="13" bold="true" />
     <color background="0xFFFFFF" />
    </style>
    <shaper class="@Rectangular" cols="2" rows="2" />
    <cell image="digestiu\estomac.gif" />
    <cell image="digestiu\fetge.gif" />
    <cell image="digestiu\pancreas.gif" />
    <cell image="digestiu\intestin.gif" />
   </cells>
   <scramble times="31" primary="true" secondary="true" />
   <layout position="AB" />
  </activity>
  <activity class="@puzzles.DoublePuzzle" name="digestiu\digestiu.puz">
   <messages>
    <cell type="initial">
     <style>
      <font family="Arial" size="13" bold="true" />
     </style>
     <p>Reordena el aparato digestivo</p>
    </cell>
    <cell type="final">
     <style>
      <font family="Arial" size="13" bold="true" />
     </style>
     <p>¡Bién!        Identifica ahora las partes</p>
    </cell>
   </messages>
   <settings margin="8" dragCells="true" report="true" reportActions="false">
    <helpWindow showSolution="true" />
    <container bgColor="0xC0C0C0">
     <counters time="true" actions="true" score="true" />
    </container>
    <window bgColor="0xC0C0C0" border="true" />
   </settings>
   <cells rows="3" cols="3" border="true" image="digestiu\digestiu.gif">
    <style>
     <font family="Arial" size="13" bold="true" />
     <color background="0xFFFFFF" />
    </style>
    <shaper class="@Rectangular" cols="3" rows="3" />
   </cells>
   <scramble times="31" />
   <layout position="AB" />
  </activity>
  <activity class="@textGrid.WordSearch" name="digestiu\digestiu.sop">
   <messages>
    <cell type="initial">
     <style>
      <font family="Arial" size="13" bold="true" />
     </style>
     <p>Localiza 7 órganos del aparato digestivo</p>
    </cell>
    <cell type="final">
     <style>
      <font family="Arial" size="13" bold="true" />
     </style>
     <p>No me lo puedo creer. ¡ Muy bien !</p>
    </cell>
   </messages>
   <settings margin="8" report="true" reportActions="false">
    <helpWindow showSolution="true" />
    <container bgColor="0xC0C0C0">
     <counters time="true" actions="true" score="true" />
    </container>
    <window bgColor="0xC0C0C0" border="true" />
   </settings>
   <textGrid rows="10" columns="9" cellWidth="40.0" cellHeight="40.0" border="true">
    <style>
     <font family="Arial" size="13" bold="true" />
     <color background="0xFFFFFF" />
    </style>
    <text>
     <row>***H***P*O</row>
     <row>VESICULA*N</row>
     <row>*S*G***N*I</row>
     <row>*O*A***C*T</row>
     <row>*F*D***R*S</row>
     <row>*ANO***E*E</row>
     <row>*G*****A*T</row>
     <row>*O*****S*N</row>
     <row>ESTOMAGO*I</row>
    </text>
   </textGrid>
   <clues>
    <clue id="0">ESOFAGO</clue>
    <clue id="1">ESTOMAGO</clue>
    <clue id="2">HIGADO</clue>
    <clue id="3">VESICULA</clue>
    <clue id="4">PANCREAS</clue>
    <clue id="5">INTESTINO</clue>
    <clue id="6">ANO</clue>
   </clues>
  </activity>
 </activities>
