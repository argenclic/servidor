  <media name="digestiu/digestiu.gif" file="digestiu_digestiu.gif" animated="false" />
  <media name="digestiu/estomac.gif" file="digestiu_estomac.gif" animated="false" />
  <media name="digestiu/fetge.gif" file="digestiu_fetge.gif" animated="false" />
  <media name="digestiu/intestin.gif" file="digestiu_intestin.gif" animated="false" />
