   <document>
    <style name="default" size="17" background="0xFFFFFF" tabWidth="16" family="Arial" bold="false" foreground="0x000000" italic="false" />
    <style name="target" base="default" target="true" background="0xC0C0C0" foreground="0x0000FF" />
    <style name="targetError" base="target" background="0xFF0000" foreground="0xFFFFFF" />
    <section>
     <p size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false">
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false">El aparato </text>
      <target>
       <answer>excretor</answer>
       <response fill="_" length="8" maxLength="8" />
       <text>excretor</text>
      </target>
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false"> está formado por: </text>
      <target>
       <answer>riñones</answer>
       <response fill="_" length="7" maxLength="7" />
       <text>riñones</text>
      </target>
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false"> y la </text>
      <target>
       <answer>vegiga</answer>
       <response fill="_" length="6" maxLength="6" />
       <text>vegiga</text>
      </target>
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false"> urinaria</text>
     </p>
     <p size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false">
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false">Los riñones filtran la sagre y forman la </text>
      <target>
       <answer>orina</answer>
       <response fill="_" length="5" maxLength="5" />
       <text>orina</text>
      </target>
     </p>
     <p size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false">
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false">La orina se acumula en la </text>
      <target>
       <answer>vegiga</answer>
       <response fill="_" length="6" maxLength="6" />
       <text>vegiga</text>
      </target>
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false"> de la orina hasta que la expulsamos al exterior.</text>
     </p>
     <p size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false">
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false">La piel tambien juega un papel importante en la excreción. Las </text>
      <target>
       <answer>glándulas</answer>
       <response fill="_" length="9" maxLength="9" />
       <text>glándulas</text>
      </target>
      <text size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false"> sudoríparas limpian la sangre y forman el </text>
      <target>
       <answer>sudor</answer>
       <response fill="_" length="5" maxLength="5" />
       <text>sudor</text>
      </target>
     </p>
    </section>
   </document>
