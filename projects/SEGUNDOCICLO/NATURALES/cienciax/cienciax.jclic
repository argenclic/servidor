   <checkButton>Evaluación</checkButton>
   <document>
    <style name="default" background="0x80FF80" family="Arial" size="17" italic="false" tabWidth="16" foreground="0x000000" bold="false" />
    <style name="target" base="default" background="0xC0C0C0" foreground="0x0000FF" target="true" />
    <style name="targetError" base="target" background="0xFF0000" foreground="0xFFFFFF" />
    <section>
     <p background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
      <text background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">En la </text>
      <target>boca</target>
      <text background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> los alimentos se trituran con los </text>
      <target>dientes</target>
      <text background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> y se mezclan con </text>
      <target>saliva</target>
      <text background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> gracias al movimiento de la </text>
      <target>lengua</target>
      <text background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">.</text>
     </p>
     <p background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000" />
     <p background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
      <text background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">El </text>
      <target>esófago</target>
      <text background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> empuja al bolo </text>
      <target>alimenticio</target>
      <text background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> hasta el estómago por medio de la </text>
      <target>contraccion</target>
      <text background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> de sus paredes musculosas.</text>
     </p>
     <p background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000" />
     <p background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
      <text background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">La válvula </text>
      <target>cardias</target>
      <text background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> está situada a la entrada del </text>
      <target>estómago</target>
      <text background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">.</text>
     </p>
     <p background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000" />
     <p background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
      <text background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">El </text>
      <target>bolo</target>
      <text background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> alimenticio es transformado en el estómago por la acción del jugo </text>
      <target>gástrico</target>
      <text background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> que lo deshace formando el </text>
      <target>quimo</target>
      <text background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">.</text>
     </p>
     <p background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000" />
     <p background="0x80FF80" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
