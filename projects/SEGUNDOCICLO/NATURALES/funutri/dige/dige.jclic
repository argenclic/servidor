   <document>
    <style name="default" size="17" background="0xFFFFFF" family="Arial" bold="false" foreground="0x000000" italic="false" tabWidth="16" />
    <style name="target" base="default" target="true" background="0xFFFFFF" foreground="0x0000FF" />
