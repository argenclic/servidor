<JClicProject name="prisma" version="0.1.3">
 <settings>
  <title>Prisma cálculo</title>
  <revision description="Creació del paquet d'activitats en Clic 3.0" date="2/2/06" />
  <revision description="Conversió de Clic3 a JClic" date="3/27/06">
