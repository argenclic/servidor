 <sequence>
  <item id="matsex.pac" name="pantmat.ass">
   <jump action="STOP" id="back" />
  </item>
  <item name="menumat.ass">
   <jump action="STOP" id="forward" />
  </item>
 </sequence>
 <activities>
  <activity class="@panels.InformationScreen" name="menumat.ass">
   <messages>
    <cell type="initial">
     <style />
    </cell>
    <cell type="final">
     <style />
    </cell>
   </messages>
   <settings margin="8" report="false">
    <container bgColor="0xFF80FF">
     <counters time="false" actions="false" score="false" />
    </container>
    <window bgColor="0xC0C0C0" border="true" />
   </settings>
   <cells rows="3" cols="1" cellWidth="250.0" cellHeight="100.0" border="true">
    <style>
     <color background="0xFFFF80" />
    </style>
    <shaper class="@Rectangular" cols="1" rows="3" />
    <cell>
     <media type="RUN_CLIC_PACKAGE" level="1" file="aritmenu.pac" params="aritmenu/aritmenu.jclic" />
     <p>ARITMÉTICA </p>
    </cell>
    <cell>
     <media type="RUN_CLIC_PACKAGE" level="1" file="meditmenu.pac" params="meditmenu/meditmenu.jclic" />
     <p>LAS MEDIDAS </p>
    </cell>
    <cell>
     <media type="RUN_CLIC_PACKAGE" level="1" file="geomenu.pac" params="geomenu/geomenu.jclic" />
     <p>GEOMETRÍA </p>
    </cell>
   </cells>
  </activity>
  <activity class="@panels.InformationScreen" name="pantmat.ass">
   <messages>
    <cell type="initial">
     <style />
     <p>Actividades de repaso y control.</p>
    </cell>
    <cell type="final">
     <style />
    </cell>
   </messages>
   <settings margin="8" report="false">
    <container bgColor="0xB0B0FF">
     <counters time="false" actions="false" score="false" />
    </container>
    <window bgColor="0xC0C0C0" border="true" />
   </settings>
   <cells rows="1" cols="1" border="true" image="portmat.gif">
    <style>
     <color background="0x80FFFF" />
    </style>
    <shaper class="@Rectangular" cols="1" rows="1" />
   </cells>
  </activity>
 </activities>
