     <size width="700" height="500" />
    </window>
   </settings>
   <prevScreen maxTime="180" />
   <document>
    <style name="default" background="0xFFFFFF" family="Arial" size="17" italic="false" tabWidth="16" foreground="0x000000" bold="false" />
    <style name="target" base="default" background="0xFFFFFF" foreground="0x0000FF" target="true" />
    <style name="targetError" base="target" background="0xFF0000" foreground="0xFFFFFF" />
    <section>
     <p Alignment="1" background="0xFFFFFF" family="Arial" size="17" italic="false" foreground="0x000000" bold="false">
      <cell width="140" height="90" border="false" image="coliseu.jpg">
       <style>
        <color background="0xFFFFFF" inactive="0xFFFFFF" alternative="0xFFFFFF" />
       </style>
      </cell>
     </p>
     <p Alignment="1" background="0xFFFFFF" family="Arial" size="17" italic="false" foreground="0x000000" bold="false">
      <text background="0xFFFFFF" family="Arial" size="17" italic="true" foreground="0x000000" bold="false">El Coliseo</text>
     </p>
     <p Alignment="1" background="0xFFFFFF" family="Arial" size="17" italic="false" foreground="0x000000" bold="false" />
     <p Alignment="1" background="0xFFFFFF" family="Arial" size="17" italic="false" foreground="0x000000" bold="false">
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" foreground="0x000000" bold="true">5- EL LEGADO ROMANO: EL DERECHO, EL IDIOMA Y EL ARTE</text>
     </p>
     <p background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000" />
     <p background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">	LA aportación más notable de Roma a la cultura occidental fue su "</text>
      <target>
       <answer>legislación</answer>
       <response fill="_" length="11" maxLength="11" />
       <text>legislación</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">" o "</text>
      <target>
       <answer>derecho</answer>
       <response fill="_" length="7" maxLength="7" />
       <text>derecho</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">". El imperio se mantuvo tantos siglos gracias a su estructura jurídica.</text>
     </p>
     <p background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">Los romanos crearon un derecho, separado de la </text>
      <target>
       <answer>moral</answer>
       <response fill="_" length="5" maxLength="5" />
       <text>moral</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">, de la </text>
      <target>
       <answer>religión</answer>
       <response fill="_" length="8" maxLength="8" />
       <text>religión</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> y tendente a la equidad entre todos los hombres idóneos.</text>
     </p>
     <p background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">Sus grandes legisladores, como </text>
      <target>
       <answer>Papiniano</answer>
       <answer>Ulpiano</answer>
       <response fill="_" length="9" maxLength="9" />
       <text>Papiniano</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> y </text>
      <target>
       <answer>Papiniano</answer>
       <answer>Ulpiano</answer>
       <response fill="_" length="7" maxLength="7" />
       <text>Ulpiano</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">, han compuesto textos claros, precisos, que se han perpetuado en los codigos civiles de los pueblos.</text>
     </p>
     <p background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">El </text>
      <target>
       <answer>latín</answer>
       <optionList>
        <option>romance</option>
        <option>latín</option>
       </optionList>
       <text>latín</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">, su idioma, tuvo una importancia extraordinaria. Con el idioma, Roma extendió su cultura, y con la difusión de su cultura se cimentó la unidad del pueblo romano.</text>
     </p>
     <p background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">Del idioma latino se originaron las "lenguas </text>
      <target>
       <answer>romances</answer>
       <response fill="_" length="8" maxLength="8" />
       <text>romances</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">" de las futuras nacionalidades europeas.</text>
     </p>
     <p background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">	Y como una muestra indeleble de su civilización, Roma nos ha dejado su </text>
      <target>
       <answer>arte</answer>
       <response fill="_" length="4" maxLength="4" />
       <text>arte</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> y sus </text>
      <target>
       <answer>construcciones</answer>
       <response fill="_" length="14" maxLength="14" />
       <text>construcciones</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> públicas, que responden al carácter práctico del genio romano.</text>
     </p>
     <p background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">	Fueron los romanos los grandes constructores de la Historia. Edificaron por todo el imperio "</text>
      <target>
       <answer>basílicas</answer>
       <response fill="_" length="9" maxLength="9" />
       <text>basílicas</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">" (para administrar justicia), "</text>
      <target>
       <answer>termas</answer>
       <response fill="_" length="6" maxLength="6" />
       <text>termas</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">" ( o baños públicos), "</text>
      <target>
       <answer>teatros</answer>
       <answer>anfiteatros</answer>
       <response fill="_" length="7" maxLength="11" />
       <text>teatros</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">", "anfiteatros", como el </text>
      <target>
       <answer>Coliseo</answer>
       <response fill="_" length="7" maxLength="7" />
       <text>Coliseo</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> de Roma, "</text>
      <target>
       <answer>circos</answer>
       <response fill="_" length="6" maxLength="6" />
       <text>circos</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">" ( para las carreras de carros), "</text>
      <target>
       <answer>acueductos</answer>
       <response fill="_" length="10" maxLength="10" />
       <text>acueductos</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">" (canales artificiales para la conducción de agua), "</text>
      <target>
       <answer>arcos</answer>
       <response fill="_" length="5" maxLength="5" />
       <text>arcos</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> del triunfo", "</text>
      <target>
       <answer>puentes</answer>
       <response fill="_" length="7" maxLength="7" />
       <text>puentes</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">" y sobre todo, "</text>
      <target>
       <answer>calzadas</answer>
       <response fill="_" length="8" maxLength="8" />
       <text>calzadas</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">" o vias romanas, las carreteras de entonces.</text>
     </p>
     <p background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">Los romanos utilizaron en sus construcciones  el "</text>
      <target>
       <answer>arco</answer>
       <response fill="_" length="4" maxLength="4" />
       <text>arco</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">" y la "</text>
      <target>
       <answer>bóveda</answer>
       <response fill="_" length="6" maxLength="6" />
       <text>bóveda</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">", desconocidos entre los </text>
      <target>
       <answer>griegos</answer>
       <optionList>
        <option>fenicios</option>
        <option>griegos</option>
       </optionList>
       <text>griegos</text>
      </target>
      <text background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">.</text>
     </p>
     <p background="0xFFFFFF" family="Arial" size="17" italic="false" bold="false" foreground="0x000000" />
    </section>
