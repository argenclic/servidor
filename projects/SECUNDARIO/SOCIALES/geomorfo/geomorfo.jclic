     <size width="638" height="212" />
    </window>
    <skin file="@blue.xml" />
   </settings>
   <prevScreen maxTime="60" />
   <document>
    <style name="default" background="0xFFCCCC" family="Arial" tabWidth="12" size="12" bold="false" italic="false" />
    <style name="target" base="default" background="0xFFFFFF" target="true" foreground="0x0000FF" />
    <style name="targetError" base="target" foreground="0xFF0000" />
    <section>
     <p background="0xFFFFFF" family="Arial">
      <text background="0xFFCCCC" family="Arial" size="14">Los </text>
      <target>
       <answer>terremotos</answer>
       <response fill="_" length="10" maxLength="10" />
       <text>terremotos</text>
      </target>
      <text background="0xFFCCCC" family="Arial" size="14">, también llamados </text>
      <target>
       <answer>seísmos</answer>
       <response fill="_" length="7" maxLength="7" />
       <text>seísmos</text>
      </target>
      <text background="0xFFCCCC" family="Arial" size="14">, están causados por los </text>
      <target>
       <answer>choques</answer>
       <response fill="_" length="7" maxLength="7" />
       <text>choques</text>
      </target>
      <text background="0xFFCCCC" family="Arial" size="14"> y </text>
      <target>
       <answer>roces</answer>
       <response fill="_" length="5" maxLength="5" />
       <text>roces</text>
      </target>
      <text background="0xFFCCCC" family="Arial" size="14"> de las placas tectónicas. Son bruscos </text>
      <target>
       <answer>movimientos</answer>
       <response fill="_" length="11" maxLength="11" />
       <text>movimientos</text>
      </target>
      <text background="0xFFCCCC" family="Arial" size="14"> de la </text>
      <target>
       <answer>corteza</answer>
       <response fill="_" length="7" maxLength="7" />
       <text>corteza</text>
      </target>
      <text background="0xFFCCCC" family="Arial" size="14"> terrestre que tienen lugar por la </text>
      <target>
       <answer>fractura</answer>
       <response fill="_" length="8" maxLength="8" />
       <text>fractura</text>
      </target>
      <text background="0xFFCCCC" family="Arial" size="14"> y </text>
      <target>
       <answer>desplazamiento</answer>
       <response fill="_" length="14" maxLength="14" />
       <text>desplazamiento</text>
      </target>
      <text background="0xFFCCCC" family="Arial" size="14"> de rocas en el interior de la Tierra.</text>
     </p>
     <p background="0xFFFFFF" family="Arial">
      <text background="0xFFCCCC" family="Arial" size="14">	La zona del interior de la Tierra donde se origina el terremoto se denomina </text>
      <target>
       <answer>hipocentro</answer>
       <response fill="_" length="10" maxLength="10" />
       <text>hipocentro</text>
      </target>
      <text background="0xFFCCCC" family="Arial" size="14"> y en la superficie se denomina </text>
      <target>
       <answer>epicentro</answer>
       <response fill="_" length="9" maxLength="9" />
       <text>epicentro</text>
      </target>
      <text background="0xFFCCCC" family="Arial" size="14">. Los terremotos </text>
      <target>
       <answer>pliegan</answer>
       <response fill="_" length="7" maxLength="7" />
       <text>pliegan</text>
      </target>
      <text background="0xFFCCCC" family="Arial" size="14"> y </text>
      <target>
       <answer>fallan</answer>
       <response fill="_" length="6" maxLength="6" />
       <text>fallan</text>
      </target>
      <text background="0xFFCCCC" family="Arial" size="14"> las estructuras terrestres y presentan distintos niveles de intensidad que son medidos a través de la escala de </text>
      <target>
       <answer>Richter</answer>
       <response fill="_" length="7" maxLength="7" />
       <text>Richter</text>
      </target>
      <text background="0xFFCCCC" family="Arial" size="14">.</text>
     </p>
     <p background="0xFFFFFF" family="Arial">
      <text background="0xFFCCCC" family="Arial" size="14">	Los pliegues y fallas se producen por efecto de las ondas </text>
      <target>
       <answer>P</answer>
       <response fill="_" length="1" maxLength="1" />
       <text>P</text>
      </target>
      <text background="0xFFCCCC" family="Arial" size="14"> y las ondas </text>
      <target>
       <answer>S</answer>
       <response fill="_" length="1" maxLength="1" />
       <text>S</text>
      </target>
      <text background="0xFFCCCC" family="Arial" size="14"> que se generan durante el seísmo. Estas ondas son las causantes de las ondulaciones del terreno y de la contracción del mismo.</text>
     </p>
     <p background="0xFFFFFF" family="Arial">
      <text background="0xFFCCCC" family="Arial" size="14">	Si el terremoto se produce en el </text>
      <target>
       <answer>océano</answer>
       <response fill="_" length="6" maxLength="6" />
       <text>océano</text>
      </target>
      <text background="0xFFCCCC" family="Arial" size="14"> las ondas dan lugar a un levantamiento de una </text>
      <target>
       <answer>ola</answer>
       <response fill="_" length="3" maxLength="3" />
       <text>ola</text>
      </target>
      <text background="0xFFCCCC" family="Arial" size="14"> gigante. Es lo que conocemos como </text>
      <target>
       <answer>Tsunami</answer>
       <response fill="_" length="7" maxLength="7" />
       <text>Tsunami</text>
      </target>
     </p>
    </section>
   </document>
