    <window bgColor="0xC0C0C0" border="true">
     <size width="697" height="300" />
    </window>
    <skin file="@blue.xml" />
   </settings>
   <checkButton>Comprueba...</checkButton>
   <prevScreen maxTime="95" />
   <document>
    <style name="default" background="0xFFFFCC" family="Comic Sans MS" tabWidth="10" size="14" bold="false" italic="false" />
    <style name="target" base="default" background="0xFFFFCC" target="true" foreground="0x336600" />
    <style name="targetError" base="target" background="0xFFFFFF" foreground="0x660000" />
    <section>
     <p background="0xFFFFFF" family="Arial">
      <text background="0xFFFFCC" family="Arial" foreground="0x000033">	</text>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033">Los </text>
      <target>volcanes</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> son colinas o montañas de forma </text>
      <target>cónica</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> construidos por la </text>
      <target>eyección</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> (EXPULSIÓN) de materiales  a través de una </text>
      <target>abertura</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> de la </text>
      <target>corteza</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> terrestre. Suelen situarse </text>
      <target>cerca</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> de los puntos de </text>
      <target>choque</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> de las </text>
      <target>placas</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> tectónicas. Así se habla de la existencia de un </text>
      <target>cinturón</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> de </text>
      <target>fuego</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> que coincide con el </text>
      <target>rozamiento</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> entre placas. Los </text>
      <target>materiales</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> que expulsan al exterior son: </text>
      <target>cenizas,</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> lapilli, </text>
      <target>bombas,</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> </text>
      <target>lava,</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> toba, fango, </text>
      <target>agua,</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> ...</text>
     </p>
     <p background="0xFFFFFF" family="Arial">
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033">	Las partes de un volcán son: </text>
      <target>cono,</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> </text>
      <target>chimenea,</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> </text>
      <target>cámara</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> magmática, </text>
      <target>cráter.</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> Cuando el cráter se erosiona se denomina </text>
      <target>caldera.</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> Suelen tener </text>
      <target>conos</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> adyacentes y/o </text>
      <target>fisuras</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> de escape de </text>
      <target>gases</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> y </text>
      <target>magma.</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> Las erupciones a veces son tan </text>
      <target>violentas</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> que producen grandes </text>
      <target>terremotos</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> y la </text>
      <target>destrucción</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> del volcán.</text>
     </p>
     <p background="0xFFFFFF" family="Arial">
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033">	Se clasifican según su </text>
      <target>actividad</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> y según su </text>
      <target>erupción.</target>
     </p>
     <p background="0xFFFFFF" family="Arial">
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033">Según su actividad nos encontramos con </text>
      <target>tres</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> tipos de volcanes:</text>
     </p>
     <p background="0xFFFFFF" family="Arial">
      <target>extinguidos,</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> </text>
      <target>dormidos</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> y </text>
      <target>activos.</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> Los volcanes extinguidos han perdido su </text>
      <target>apariencia</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> cónica por la fuerte </text>
      <target>erosión</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> que han sufrido sus </text>
      <target>calderas</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> a lo largo de los siglos.</text>
     </p>
     <p background="0xFFFFFF" family="Arial">
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033">Según el tipo de </text>
      <text background="0xFFFFCC" family="Comic Sans MS">erupción</text>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> los </text>
      <target>clasificamos</target>
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033"> en:</text>
     </p>
     <p background="0xFFFFFF" family="Arial">
      <text background="0xFFFFCC" family="Comic Sans MS" foreground="0x000033">Hawaiano, peleano, stromboliano, vulcaniano, maar y vesubiano</text>
     </p>
    </section>
   </document>
