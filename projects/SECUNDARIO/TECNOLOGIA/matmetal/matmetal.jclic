   <prevScreen maxTime="60" />
   <document>
    <style name="default" background="0xFF8080" family="Arial" size="17" italic="false" tabWidth="16" bold="false" foreground="0x000000" />
    <style name="target" base="default" background="0xC0C0C0" target="true" foreground="0x0000FF" />
    <style name="targetError" base="target" background="0xFF0000" foreground="0xFFFFFF" />
    <section>
     <p background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">EL COBRE</text>
     </p>
     <p background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">El cobre  es un metal de color </text>
      <target>
       <answer>rojo</answer>
       <response fill="_" length="4" maxLength="4" />
       <text>rojo</text>
      </target>
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> brillante, muy resistente a la </text>
      <target>
       <answer>corrosión</answer>
       <response fill="_" length="9" maxLength="9" />
       <text>corrosión</text>
      </target>
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">, conduce bien el </text>
      <target>
       <answer>calor</answer>
       <response fill="_" length="5" maxLength="5" />
       <text>calor</text>
      </target>
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> y la </text>
      <target>
       <answer>electricidad</answer>
       <response fill="_" length="12" maxLength="12" />
       <text>electricidad</text>
      </target>
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">, es muy dúctil y </text>
      <target>
       <answer>maleable</answer>
       <response fill="_" length="8" maxLength="8" />
       <text>maleable</text>
      </target>
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">. Se obtiene de minerales como la cuprita, la </text>
      <target>
       <answer>calcopirita</answer>
       <response fill="_" length="11" maxLength="11" />
       <text>calcopirita</text>
      </target>
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> y la malaquita.</text>
     </p>
     <p background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">Se ha usado desde la antigüedad para hacer </text>
      <target>
       <answer>armas</answer>
       <response fill="_" length="5" maxLength="5" />
       <text>armas</text>
      </target>
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">, adornos, monedas, etc. Hoy se usa en </text>
      <target>
       <answer>conductores</answer>
       <response fill="_" length="11" maxLength="11" />
       <text>conductores</text>
      </target>
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> </text>
      <target>
       <answer>eléctricos</answer>
       <response fill="_" length="10" maxLength="10" />
       <text>eléctricos</text>
      </target>
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">, alambiques, y conducciones de gas y </text>
      <target>
       <answer>agua</answer>
       <response fill="_" length="4" maxLength="4" />
       <text>agua</text>
      </target>
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">, así como otros usos en construcción. Sus aleaciones principales son:</text>
     </p>
     <p background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">Los </text>
      <target>
       <answer>bronces</answer>
       <response fill="_" length="7" maxLength="7" />
       <text>bronces</text>
      </target>
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">. Aleaciones de cobre y </text>
      <target>
       <answer>estaño</answer>
       <response fill="_" length="6" maxLength="6" />
       <text>estaño</text>
      </target>
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">, tanto más duras cuanto más </text>
      <target>
       <answer>estaño</answer>
       <response fill="_" length="6" maxLength="6" />
       <text>estaño</text>
      </target>
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> contienen.</text>
     </p>
     <p background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">Los </text>
      <target>
       <answer>latones</answer>
       <response fill="_" length="7" maxLength="7" />
       <text>latones</text>
      </target>
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">. Aleaciones de cobre y </text>
      <target>
       <answer>cinc</answer>
       <response fill="_" length="4" maxLength="4" />
       <text>cinc</text>
      </target>
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> usadas para hacer canalizaciones, tornillos, válvulas de gas y agua, bisagras, etc..</text>
     </p>
     <p background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000" />
    </section>
   </document>
