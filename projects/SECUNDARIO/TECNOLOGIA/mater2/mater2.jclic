      <target>
       <answer>Gastar</answer>
       <optionList>
        <option>Gastar</option>
        <option>Ahorrar</option>
        <option>Poseer</option>
       </optionList>
       <text>Gastar</text>
      </target>
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> menos materiales.</text>
     </p>
     <p background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
      <target>
       <answer>Reciclar</answer>
       <optionList>
        <option>Derrochar</option>
        <option>Consumir</option>
        <option>Reciclar</option>
       </optionList>
       <text>Reciclar</text>
      </target>
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000"> todos los que sea posible.</text>
     </p>
     <p background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">
      <text background="0xFF8080" family="Arial" size="17" italic="false" bold="false" foreground="0x000000">Reciclar ahorra </text>
