    <style>
     <color background="0x8080FF" inactive="0xFFFFFF" alternative="0xFFFFFF" />
    </style>
    <text>
     <row>*P*N*Y***************</row>
     <row>*I*A*E**GREEN****O***</row>
     <row>*N*V*L**R********R***</row>
     <row>WK*Y*L**E********A**B</row>
     <row>H**-*O**Y*BLUE***N**R</row>
     <row>I**B*W***********G**O</row>
     <row>T**L*************E**W</row>
     <row>E**U*RED************N</row>
     <row>***E*******PURPLE****</row>
     <row>******BLACK****LILAC*</row>
    </text>
   </textGrid>
   <clues>
    <clue id="0">RED</clue>
    <clue id="1">YELLOW</clue>
    <clue id="2">BLUE</clue>
    <clue id="3">ORANGE</clue>
    <clue id="4">BLACK</clue>
    <clue id="5">GREEN</clue>
