  <activity class="@textGrid.WordSearch" name="ingles.sop" code="">
   <description>
    <p>Buscar nombre de colores escrito en ingles</p>
   </description>
   <messages>
    <cell type="initial">
     <style />
     <p>Busca 13 colores escrito en ingles</p>
    </cell>
