  <activity class="@text.FillInBlanks" name="six.txa">
   <messages>
    <cell type="initial">
     <style />
     <p>Fill in the blanks the correct possessive form.</p>
    </cell>
    <cell type="final">
     <style />
     <media type="PLAY_AUDIO" level="2" file="great!.wav" />
    </cell>
   </messages>
   <settings margin="8" report="true" reportActions="false">
    <helpWindow showSolution="true" />
    <container bgColor="0xFFFF00">
     <counters time="true" actions="true" score="true" />
    </container>
    <window bgColor="0xC0C0C0" border="true">
     <size width="540" height="330" />
    </window>
