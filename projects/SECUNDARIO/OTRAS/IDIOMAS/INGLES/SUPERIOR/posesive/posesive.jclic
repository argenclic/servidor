  <activity class="@textGrid.WordSearch" name="five.sop">
   <messages>
    <cell type="initial">
     <style />
     <p>Find the hidden possessive adjectives and pronouns. There are eight.</p>
    </cell>
    <cell type="final">
     <style />
     <media type="PLAY_AUDIO" level="2" file="great!.wav" />
    </cell>
   </messages>
   <settings margin="8" report="true" reportActions="false">
    <helpWindow showSolution="true" />
    <container bgColor="0xFF0080">
     <counters time="true" actions="true" score="true" />
    </container>
    <window bgColor="0xC0C0C0" border="true" />
   </settings>
   <textGrid rows="8" columns="8" cellWidth="300.0" cellHeight="300.0" border="true">
    <style>
     <color background="0xFFFFFF" />
    </style>
    <text>
     <row>***RUO**</row>
     <row>***MY*I*</row>
     <row>****O*T*</row>
     <row>****U*S*</row>
     <row>THEIR***</row>
     <row>****SREH</row>
     <row>*******I</row>
     <row>MINE***S</row>
    </text>
   </textGrid>
   <clues>
    <clue id="0">OUR</clue>
    <clue id="1">MY</clue>
    <clue id="2">THEIR</clue>
    <clue id="3">MINE</clue>
    <clue id="4">HERS</clue>
    <clue id="5">ITS</clue>
    <clue id="6">HIS</clue>
    <clue id="7">YOURS</clue>
   </clues>
  </activity>
