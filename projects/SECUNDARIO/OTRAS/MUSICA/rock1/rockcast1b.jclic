    <cell id="5" image="Gloria Gaynor.jpg">
     <media type="PLAY_AUDIO" level="1" file="Gloria Gaynor - I Will Survive (79).mp3" />
    </cell>
    <cell id="6" image="Prince2.jpg">
     <media type="PLAY_AUDIO" level="1" file="Prince - Purple Rain (84).mp3" />
