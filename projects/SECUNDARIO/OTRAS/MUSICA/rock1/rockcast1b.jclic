    <shaper class="@Rectangular" cols="1" rows="5" />
    <cell>
     <p>El pogo</p>
    </cell>
    <cell>
     <p>El look pobre</p>
    </cell>
    <cell>
     <p>Sex Pistols, The Clash, Ramones ...</p>
    </cell>
    <cell>
     <p>Anarquía</p>
    </cell>
    <cell>
     <p>El glam rock o rock decadente</p>
    </cell>
   </cells>
