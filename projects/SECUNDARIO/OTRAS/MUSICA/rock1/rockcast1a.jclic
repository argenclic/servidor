    </style>
    <shaper class="@Rectangular" cols="1" rows="4" />
    <cell id="0" image="semilla.jpg">
     <media type="PLAY_AUDIO" level="1" file="Bill Haley &amp; The Comets -12 comp Rock Around The Cock (55).mp3" />
    </cell>
    <cell image="sun.jpg" />
