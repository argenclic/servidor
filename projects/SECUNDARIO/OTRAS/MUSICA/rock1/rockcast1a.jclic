    <shaper class="@Rectangular" cols="1" rows="4" />
    <cell>
     <p>El folk</p>
    </cell>
    <cell>
     <p>Los Festivales</p>
    </cell>
    <cell>
     <p>La psicodelia</p>
    </cell>
    <cell>
