  <activity class="@panels.InformationScreen" name="znorte.ass" code="">
   <description />
   <messages>
    <cell type="initial">
     <style>
      <font family="Times New Roman" size="30" bold="true" />
      <color foreground="0x400040" background="0xCECEFF" />
     </style>
     <p>Lee e identificarás el Folcklore de la Zona Norte. </p>
    </cell>
    <cell type="final">
     <style>
      <font family="Times New Roman" size="30" bold="true" />
      <color foreground="0x400040" background="0xCECEFF" />
     </style>
    </cell>
   </messages>
   <settings margin="8" report="false">
    <container bgColor="0xFFC8FF">
     <counters time="false" actions="false" score="false" />
