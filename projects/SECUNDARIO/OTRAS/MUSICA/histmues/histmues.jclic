   <prevScreen />
   <document>
    <style name="default" background="0xF7F8BA" bold="true" size="18" italic="false" tabWidth="15" foreground="0x008080" family="dialog" />
    <style name="target" base="default" background="0xC0C0C0" target="true" foreground="0x0000FF" />
    <style name="targetError" base="target" background="0xFF0000" foreground="0xFFFFFF" />
    <section>
     <p background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog">
      <cell width="140" height="100" border="false" image="bach.gif">
       <style>
        <color background="0xF7F8BA" />
       </style>
       <media type="PLAY_AUDIO" level="1" file="bach3mp3.mp3" />
      </cell>
      <text background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog">   ¿DE QUÉ VA ESTA HISTORIA?</text>
     </p>
     <p background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog" />
     <p background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog">
      <text background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog">EN LA PREHISTORIA, LA MÚSICA ESTABA RELACIONADA CON LA RELIGIÓN, LA </text>
      <target>
       <answer>MÀGIA</answer>
       <optionList>
        <option>MÀGIA</option>
        <option>POR</option>
        <option>PLUJA</option>
       </optionList>
       <text>MAGIA</text>
      </target>
      <text background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog"> Y EL DISFRUTE. SIN EMBARGO, NO TENEMOS BASTANTES ELEMENTOS PARA SABER CÓMO SONABA EXACTAMENTE. </text>
     </p>
     <p background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog" />
     <p background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog">
      <text background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog">EN LA EDAD ANTIGUA, DESTACA LA CIVILIZACIÓN  </text>
      <target>GRIEGA</target>
      <text background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog">, BASE DE TODA LA CULTURA OCCIDENTAL CRISTIANA.</text>
     </p>
     <p background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog" />
     <p background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog">
      <text background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog">EN LA EDAD MEDIA, NACE EL CANTO </text>
      <target>GREGORIANO</target>
      <text background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog">, LA POLIFONÍA Y LA MÚSICA TROVADORESCA. FORMA INSTRUMENTAL: ESTAMPIDA.</text>
     </p>
     <p background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog" />
     <p background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog">
      <text background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog">DURANTE EL RENACIMIENTO, LA MÚSICA TODAVÍA SE BASA EN LOS MODOS ANTIGUOS, PERO SE VA LIBERANDO. PREDOMINA LA MÚSICA VOCAL (RELIGIOSA-MISAS Y MOTETES- Y PROFANA-MADRIGALES-). ALGUNOS INSTRUMENTOS SE CONSTITUYEN EN </text>
      <target>FAMILIAS</target>
      <text background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog">.</text>
     </p>
     <p background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog" />
     <p background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog">
      <text background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog">AL BARROC, LA MÚSICA TÉ UNA FUNCIÓ PROPAGANDÍSTICA DEL PODER CIVIL I RELIGIÓS. EXPRESSIVITAT I ARTIFICI. PREDOMINI DE L'ESCRIPTURA HARMÒNICA. LA MÚSICA INSTRUMENTAL (CONCERTO GROSSO I CONCERT PER A SOLISTA) S'INDEPENDITZA DE LA VOCAL (SONATA PER A SOLISTA I BAIX CONTINU I SONATA A TRIO). NAIXEMENT DE L'</text>
      <target>ÒPERA</target>
      <text background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog">.</text>
     </p>
     <p background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog">
      <text background="0xF7F8BA" bold="true" size="18" italic="false" foreground="0x008080" family="dialog"> </text>
     </p>
    </section>
   </document>
   <evaluator class="@ComplexEvaluator" checkCase="true" />
  </activity>
