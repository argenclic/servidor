    <style name="target" base="default" target="true" background="0xC0C0C0" foreground="0x0000FF" />
    <style name="targetError" base="target" background="0xFF0000" foreground="0xFFFFFF" />
    <section>
     <p>
      <target>
       <answer>La masa</answer>
       <optionList>
        <option>La masa</option>
        <option>El peso</option>
       </optionList>
       <text />
      </target>
     </p>
     <p>
      <target>
       <answer>La masa</answer>
       <optionList>
        <option>La masa</option>
        <option>El peso</option>
       </optionList>
       <text>La masa</text>
      </target>
      <text> de un cuerpo es siempre igual.</text>
     </p>
     <p>
      <text>En cambio </text>
      <target>
       <answer>el peso</answer>
       <optionList>
        <option>la masa</option>
        <option>el peso</option>
       </optionList>
       <text>el peso</text>
      </target>
      <text> varía:</text>
     </p>
     <p>
      <text>	Según </text>
      <target>
       <answer>la masa</answer>
       <optionList>
        <option>la densidad</option>
        <option>el volumen</option>
        <option>la masa</option>
        <option>el color</option>
        <option>la temperatura</option>
       </optionList>
       <text>la masa</text>
      </target>
      <text> del cuerpo.</text>
     </p>
     <p>
      <text>	Según </text>
      <target>
       <answer>el planeta</answer>
       <optionList>
        <option>el planeta</option>
        <option>el estado</option>
       </optionList>
       <text>el planeta</text>
      </target>
      <text> en que se encuentre el cuerpo.</text>
     </p>
     <p>
      <text>	Si el cuerpo está en la Tierra, según </text>
      <target>
       <answer>la altura</answer>
       <optionList>
        <option>la distancia al polo</option>
        <option>el estado</option>
        <option>la altura</option>
       </optionList>
       <text>la altura</text>
      </target>
      <text> a la que esté el cuerpo.</text>
     </p>
    </section>
   </document>
