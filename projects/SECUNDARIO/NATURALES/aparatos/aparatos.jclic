<JClicProject name="coshuma" version="0.1.3">
 <settings>
  <title>Aparatos del cuerpo humano</title>
  <revision description="Creación del paquete de actividades en Clic 3.0" date="4/4/00" />
  <revision description="Traducción Español" date="5/25/00">
   <author name="Aurelio Salvador Baselga" mail="asalvado@roble.pntic.mec.es" />
  </revision>
