 </settings>
 <sequence>
  <item id="coshuma.pac" name="menu.ass">
   <jump action="STOP" id="forward" />
   <jump action="STOP" id="back" />
  </item>
 </sequence>
 <activities>
  <activity class="@panels.InformationScreen" name="menu.ass">
   <messages>
    <cell type="initial">
     <style shadow="true">
      <color foreground="0xFF0000" background="0x000000" />
     </style>
     <p>ESCOGE EL PAQUETE DE ACTIVIDADES QUE QUIERAS.</p>
    </cell>
    <cell type="final">
     <style shadow="true">
      <color foreground="0xFF0000" background="0x000000" />
     </style>
    </cell>
   </messages>
   <settings margin="8" report="false">
    <container bgColor="0x000000">
     <counters time="false" actions="false" score="false" />
    </container>
    <window bgColor="0x000000" border="false" />
   </settings>
   <cells rows="2" cols="3" cellWidth="175.0" cellHeight="175.0" border="true">
    <style>
     <color background="0x000000" />
    </style>
    <shaper class="@Rectangular" cols="3" rows="2" />
    <cell image="mendig1.gif">
     <media type="RUN_CLIC_PACKAGE" level="1" file="digest.pac" params="digest/digest.jclic" />
    </cell>
    <cell image="mencor.gif">
     <media type="RUN_CLIC_PACKAGE" level="1" file="circula.pac" params="circula/circula.jclic" />
    </cell>
    <cell image="mencrani.gif">
     <media type="RUN_CLIC_PACKAGE" level="1" file="locom.pac" params="locom/locom.jclic" />
    </cell>
    <cell image="menexcr1.gif">
     <media type="RUN_CLIC_PACKAGE" level="1" file="excre.pac" params="excre/excre.jclic" />
    </cell>
    <cell image="menrepr.gif">
     <media type="RUN_CLIC_PACKAGE" level="1" file="reprod.pac" params="reprod/reprod.jclic" />
    </cell>
    <cell image="menresp1.gif">
     <media type="RUN_CLIC_PACKAGE" level="1" file="resp.pac" params="resp/resp.jclic" />
    </cell>
   </cells>
  </activity>
 </activities>
