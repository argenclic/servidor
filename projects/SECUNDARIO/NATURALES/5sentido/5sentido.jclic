   <document>
    <style name="default" italic="false" bold="true" background="0xFF80C0" tabWidth="13" foreground="0x000000" family="Garamond" size="22" />
    <style name="target" base="default" background="0xC0C0C0" target="true" foreground="0x0000FF" />
