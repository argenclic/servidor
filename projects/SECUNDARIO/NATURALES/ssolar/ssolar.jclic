    <section>
     <p>
      <text>La Luna:</text>
     </p>
     <p />
     <p>
      <text>Es el único </text>
      <target>
       <answer>satélite</answer>
       <response fill="_" length="8" maxLength="8" />
       <info delay="2">
        <cell hAlign="left" width="140" height="100">
