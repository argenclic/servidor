  <activity class="@associations.ComplexAssociation" name="funcio1/aso_ecu.ass">
   <messages>
    <cell type="initial">
     <style>
      <font family="Arial" size="13" bold="true" />
     </style>
     <p>Asocia cada ecuación a su gráfica (clica sobre el nombre de cada recta).</p>
    </cell>
    <cell type="final">
     <style>
      <font family="Arial" size="13" bold="true" />
     </style>
     <p>¡Enhorabuena!</p>
    </cell>
   </messages>
   <settings margin="8" report="true" reportActions="false">
    <container bgColor="0xC0C0C0">
     <counters time="true" actions="true" score="true" />
    </container>
    <window bgColor="0xC0C0C0" border="true" />
   </settings>
   <cells rows="6" cols="1" cellWidth="75.0" cellHeight="60.0" border="true" id="primary">
    <style>
     <font family="Arial" size="13" bold="true" />
     <color background="0xFFFFFF" />
    </style>
    <shaper class="@Rectangular" cols="1" rows="6" />
    <cell id="0">
     <p>y=-x-1</p>
    </cell>
    <cell id="1">
     <p>y=-4x-3</p>
    </cell>
    <cell id="2">
     <p>y=5x+1</p>
    </cell>
    <cell id="3">
     <p>y=x/3+1</p>
    </cell>
    <cell id="4">
     <p>y=x/3-3</p>
    </cell>
    <cell id="5">
     <p>y=-3</p>
    </cell>
   </cells>
   <cells rows="6" cols="6" border="false" image="funcio1/recta_01.gif" id="secondary">
    <style>
     <font family="Arial" size="13" bold="true" />
     <color background="0xFFFFFF" />
    </style>
    <shaper class="@Holes" cols="6" rows="6" scaleX="6.0" scaleY="6.0">
