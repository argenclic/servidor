     <shape>rectangle:2,0,1,1</shape>
     <shape>rectangle:3,0,1,1</shape>
     <shape>rectangle:5,1,1,1</shape>
     <shape>rectangle:5,3,1,1</shape>
     <shape>rectangle:5,4,1,1</shape>
    </shaper>
